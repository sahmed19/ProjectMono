{
	"root": "",
	"references":
	[

	],
	"content":
	{
		"graphics.characters":
		{
			"path": "graphics/characters/*.png",
			"recursive": "True",
			"action": "build",
			"importer": "TextureImporter",
			"processor": "TextureProcessor",
			"processorParam":
			{
				"ColorKeyColor": "255,0,255,255",
				"ColorKeyEnabled": "True",
				"GenerateMipmaps": "False",
				"PremultiplyAlpha": "True",
				"ResizeToPowerOfTwo": "False",
				"MakeSquare": "False",
				"TextureFormat": "Color"
			}
		},
		"graphics.maps":
		{
			"path": "graphics/maps/*.png",
			"recursive": "True",
			"action": "build",
			"importer": "TextureImporter",
			"processor": "TextureProcessor",
			"processorParam":
			{
				"ColorKeyColor": "255,0,255,255",
				"ColorKeyEnabled": "True",
				"GenerateMipmaps": "False",
				"PremultiplyAlpha": "True",
				"ResizeToPowerOfTwo": "False",
				"MakeSquare": "False",
				"TextureFormat": "Color"
			}
		},
		"graphics.shaders":
		{
			"path": "graphics/shaders/*.fx",
			"recursive": "True",
			"action": "build",
			"importer": "EffectImporter",
			"processor": "EffectProcessor"
		},
		"data.tilemaps":
		{
			"path": "data/tilemaps/*.tmx",
			"recursive": "True",
			"action": "copy"
		},
		"data.tilesets":
		{
			"path": "data/tilesets/*.tsx",
			"recursive": "True",
			"action": "copy"
		}
